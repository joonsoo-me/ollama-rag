# Environment variables for ollama-rag-gateway
# Copy this file to .env and fill in your actual service URLs

# External service URLs (required)
OLLAMA_URL=http://your-ollama-server:11434
N8N_PREPARE_URL=http://your-n8n-server:5678/webhook/rag/prepare
N8N_UPSERT_URL=http://your-n8n-server:5678/webhook/rag/upsert

# Gateway configuration (optional - defaults shown)
APP_BIND_PORT=11434
RAG_TOPK=12
RERANK_TOPN=5
SCORE_THRESHOLD=0.2

# Example URLs for local development or specific deployments:
# OLLAMA_URL=http://192.168.1.100:11434
# N8N_PREPARE_URL=http://192.168.1.200:5678/webhook/rag/prepare
# N8N_UPSERT_URL=http://192.168.1.200:5678/webhook/rag/upsert
